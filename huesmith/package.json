{
  "name": "huesmith",
  "displayName": "Huesmith",
  "description": "Forge your code in vibrant hues.",
  "publisher": "Eucleiance",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.1"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "huesmith.updateColor",
      "title": "Huesmith: Update Color"
    }],
    "configuration": {
      "type": "object",
      "title": "Huesmith",
      "properties": {
        "PrimaryBackgroundColor.Enable": {
          "title": "Hello",
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Primary Background Color",
          "order": 1
        },
        "PrimaryBackground.HexCode": {
          "type": "string",
          "default": "#0d0d0d",
          "description": "Editor Background, Side Bar, Title Bar, Status Bar, Terminal...",
          "order": 2
        },
        "PrimaryForegroundColor.Enable": {
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Primary Foreground Color",
          "order": 3

        },
        "PrimaryForegroundColor.HexCode": {
          "type": "string",
          "default": "#0d0d0d",
          "description": "Title Bar Text, Directory/File Names, Tab Names, Breadcrumbs... ",
          "order": 4
        },
        "PrimaryForegroundColor [Bold].Enable": {
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Primary Foreground Color",
          "order": 5

        },
        "PrimaryForegroundColor [Bold].HexCode": {
          "type": "string",
          "default": "#0d0d0d",
          "description": "Title Bar Text, Directory/File Names, Tab Names, Breadcrumbs... ",
          "order": 6
        },
        "CursorColor.Enable": {
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Cursor Color",
          "order": 7

        },
        "CursorColor.HexCode": {
          "type": "string",
          "default": "#FFFFFFAA",
          "description": "Title Bar Text, Directory/File Names, Tab Names, Breadcrumbs... ",
          "order": 8
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}