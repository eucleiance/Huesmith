{
  "name": "huesmith",
  "displayName": "Huesmith",
  "description": "Forge your code in vibrant hues.",
  "publisher": "Eucleiance",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "huesmith.updateColor",
        "title": "Huesmith: Update Color"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Huesmith",
      "properties": {
        "PrimaryBackground.Enable": {
          "title": "Hello",
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Primary Background Color",
          "order": 1
        },
        "PrimaryBackground.HexCode": {
          "type": "string",
          "default": "#0A0A0C",
          "description": "Editor Background, Side Bar, Title Bar, Status Bar, Terminal...",
          "order": 2
        },
        "SecondaryBackground.Enable": {
          "title": "Hello",
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Secondary Background Color",
          "order": 3
        },
        "SecondaryBackground.HexCode": {
          "type": "string",
          "default": "#121218",
          "description": "Editor Background, Side Bar, Title Bar, Status Bar, Terminal...",
          "order": 4
        },
        "PrimaryForeground.Enable": {
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Primary Foreground Color",
          "order": 5
        },
        "PrimaryForeground.HexCode": {
          "type": "string",
          "default": "#FFFFFF",
          "description": "Active Title Bar Text, Active Directory/File Names, Active Tab Names... ",
          "order": 6
        },
        "SecondaryForeground.Enable": {
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Secondary Foreground Color",
          "order": 7
        },
        "SecondaryForeground.HexCode": {
          "type": "string",
          "default": "#77779F",
          "description": "Title Bar Text, Directory/File Names, Tab Names, Breadcrumbs... ",
          "order": 8
        },
        "Borders.Enable": {
          "type": "boolean",
          "default": false,
          "description": "Enable Custom Border Color",
          "order": 9
        },
        "Borders.HexCode": {
          "type": "string",
          "default": "#000000",
          "description": "HexCode for Borders.",
          "order": 10
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}